{
  "version": 2,
  "builds": [
    { "src": "public/**", "use": "@vercel/static" },
    { "src": "api/spec-to-config.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "^/api/spec-to-config$", "dest": "/api/spec-to-config.js" },
    { "src": "^/$", "dest": "/public/index.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
} 